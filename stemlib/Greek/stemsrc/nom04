:wd:e(h=s rel_pron gen sg fem epic
:wd:e(oi= rel_pron dat sg masc neut epic
:wd:o(/nde adverb acc sg masc epic
:wd:o(/s   relative masc nom sg
:wd:h(/   relative fem nom sg attic homeric ionic
:wd:a(_/   relative fem nom sg doric aeolic
:wd:ou(=   relative masc/neut gen sg
:wd:oi(=o   relative masc/neut gen sg homeric
:wd:w(=   relative masc/neut gen sg doric
:wd:h(=s   relative fem gen sg attic homeric ionic
:wd:a(=s   relative fem gen sg doric aeolic
:wd:w(=|   relative masc/neut dat sg
:wd:h(=|   relative fem dat sg attic homeric ionic
:wd:a(=|   relative fem dat sg doric aeolic
:wd:o(/n   relative masc acc sg
:wd:h(/n   relative fem acc sg attic homeric ionic
:wd:a(_/n   relative fem acc sg doric aeolic
:wd:o(/n   relative neut acc sg
:wd:w(/   relative masc nom dual
:wd:a(_/   relative fem nom dual
:wd:oi(=n   relative masc/neut gen/dat dual
:wd:ai(=n   relative fem gen/dat dual
:wd:w(/   relative masc/neut acc dual
:wd:a(_/   relative fem acc dual
:wd:oi(/   relative masc nom pl
:wd:ai(/   relative fem nom pl
:wd:a(/   relative neut nom pl
:wd:w(=n   relative masc/neut gen pl
:wd:e(/wn   relative masc/fem gen pl ionic
:wd:a(=n   relative masc/fem gen pl doric
:wd:a(/wn   relative masc/fem gen pl homeric doric
:wd:w(=n   relative fem gen pl
:wd:oi(=s   relative masc/neut dat pl
:wd:oi(=si   relative masc/neut dat pl homeric ionic aeolic
:wd:oi(=sin   relative masc/neut dat pl homeric ionic aeolic nu_movable
:wd:ai(=si   relative fem dat pl poetic
:wd:ai(=sin   relative fem dat pl nu_movable poetic
:wd:ai(=s   relative fem dat pl
:wd:h(=|sin   relative fem dat pl homeric ionic nu_movable
:wd:h(=|si   relative fem dat pl homeric ionic
:wd:ou(/s   relative masc acc pl
:wd:w(/s   relative masc acc pl doric
:wd:a(_/s   relative fem acc pl
:wd:a(/   relative neut acc pl


